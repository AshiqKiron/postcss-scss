{
  "name": "postcss-scss",
  "version": "0.4.1",
  "description": "SCSS parser for PostCSS",
  "keywords": [
    "css",
    "postcss",
    "postcss-syntax",
    "parser",
    "scss",
    "sass"
  ],
  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "license": "MIT",
  "repository": "postcss/postcss-scss",
  "dependencies": {
    "postcss": "^5.2.13"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-precompile-charcodes": "1.0.0",
    "babel-preset-es2015": "^6.22.0",
    "del": "2.2.2",
    "eslint": "^3.15.0",
    "eslint-config-postcss": "2.0.2",
    "gulp": "3.9.1",
    "gulp-babel": "6.1.2",
    "gulp-changed": "1.3.2",
    "gulp-eslint": "3.0.1",
    "gulp-jest": "^1.0.0",
    "gulp-json-editor": "2.2.1",
    "gulp-sourcemaps": "^2.4.1",
    "jest": "^19.0.2",
    "lint-staged": "^3.3.0",
    "postcss-parser-tests": "^5.0.11",
    "pre-commit": "^1.2.2",
    "run-sequence": "1.2.2"
  },
  "scripts": {
    "lint-staged": "lint-staged",
    "test": "gulp"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "eslint-config-postcss",
    "rules": {
      "key-spacing": [
        "error",
        {
          "align": "value"
        }
      ],
      "complexity": "off"
    },
    "env": {
      "jest": true
    }
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "loose": true
        }
      ]
    ],
    "plugins": [
      "add-module-exports",
      "precompile-charcodes"
    ]
  },
  "main": "lib/scss-syntax",
  "lint-staged": {
    "test/*.js": "eslint",
    "lib/*.es6": "eslint"
  },
  "pre-commit": [
    "lint-staged"
  ]
}
